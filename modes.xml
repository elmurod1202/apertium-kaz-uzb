<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <!-- kaz → uzb -->

  <mode name="kaz-uzb" install="yes" gendebug="yes">
    <pipeline>

      <program name="lt-proc -w">
        <file name="kaz-uzb.automorf.bin"/>
      </program>


      <program name="cg-proc -w -1 -n">
        <file name="kaz-uzb.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="kaz-uzb.autobil.bin"/>
      </program>
      <program name="lrx-proc -m">
        <file name="kaz-uzb.autolex.bin"/>
      </program>

      <program name="apertium-transfer -b">
        <file name="apertium-kaz-uzb.kaz-uzb.t1x"/>
        <file name="kaz-uzb.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-kaz-uzb.kaz-uzb.t2x"/>
        <file name="kaz-uzb.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-kaz-uzb.kaz-uzb.t3x"/>
        <file name="kaz-uzb.t3x.bin"/>
      </program>

      <program name="lt-proc $1">
        <file name="kaz-uzb.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="kaz-uzb.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

  <!-- uzb → kaz -->

  <mode name="uzb-kaz" install="yes" gendebug="yes">
    <pipeline>

      <program name="lt-proc -w">
        <file name="uzb-kaz.automorf.bin"/>
      </program>


      <program name="cg-proc -w -1 -n">
        <file name="uzb-kaz.rlx.bin"/>
      </program>

      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="uzb-kaz.autobil.bin"/>
      </program>
      <program name="lrx-proc -m">
        <file name="uzb-kaz.autolex.bin"/>
      </program>

      <program name="apertium-transfer -b">
        <file name="apertium-kaz-uzb.uzb-kaz.t1x"/>
        <file name="uzb-kaz.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-kaz-uzb.uzb-kaz.t2x"/>
        <file name="uzb-kaz.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-kaz-uzb.uzb-kaz.t3x"/>
        <file name="uzb-kaz.t3x.bin"/>
      </program>

      <program name="lt-proc $1">
        <file name="uzb-kaz.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="uzb-kaz.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>
</modes>

